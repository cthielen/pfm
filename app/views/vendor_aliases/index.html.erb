<p id="notice"><%= notice %></p>

<h1>Listing Vendor Aliases</h1>

<table>
  <thead>
    <tr>
      <th>Alias</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vendor_aliases.each do |vendor_alias| %>
      <tr>
        <td><%= vendor_alias.alias %></td>
        <td><%= link_to 'Show', vendor_alias %></td>
        <td><%= link_to 'Edit', edit_vendor_alias_path(vendor_alias) %></td>
        <td><%= link_to 'Destroy', vendor_alias, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vendor alias', new_vendor_alias_path %>
